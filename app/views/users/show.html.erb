<h1>Listing users</h1>
 
<table>
  <tr>
    <th>Email</th>
    <th>Role</th>
    <th></th>
  </tr>

  <% @users.each do |user| %>
    <tr>
      <td><%= user.email %></td>
      <td><%= user.role %></td>
      <td><%= link_to 'Show', user_path(user) %></td>
    </tr>
  <% end %>
</table>
<br/>
<%= link_to 'New user', new_user_session_path, :class => "btn btn-primary" %>