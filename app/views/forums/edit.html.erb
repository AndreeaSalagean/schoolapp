<%= render 'chapters/chapter' %>
 <div class="col-sm-9">
   <h4><small>Edit Forum</small></h4>
      <hr><p>
 
<%= form_tag '/forums/update' do %>
   <p>
    <%= label_tag :forum_title %><br>
    <%= text_field_tag 'forum[title]' %>
  </p>

  <p>
    <%= label_tag :description %><br>
    <%= text_field_tag 'forum[description]' %>
  </p>
  
<%= submit_tag("create") %>
<% end %>
