<!doctype html>
<html lang="en">
  <div class="jumbotron hero-nature bg-dark text-white vertical-center" >
  <div class="container text-center">
  <head>
    <title><%=  %></title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
